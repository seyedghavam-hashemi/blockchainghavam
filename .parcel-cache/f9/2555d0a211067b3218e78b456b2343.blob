body {
    background-color: #a52a2a;
    color: #fff;
    text-align: center;
    font-size: 20px;
    font-family: 'Quiksand';
    padding-top: 5%;
    word-wrap: break-word;
}

.logo {
    width: 250px;
    height: 250px;    
}

.App {
    display: flex;
    flex-direction: column;
    align-items:center;
    background-color: #ffff00;
}
.WalletInfo {
    width: 500px;
}

.Block {
    background-color: #ffff00;
    border: 1px solid #555;
    padding: 5%;
    margin: 2%;
}

